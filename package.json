{
  "name": "notes-api",
  "version": "2.2.0",
  "description": "A Node.js starter for Serverless Framework with ES6 and TypeScript support",
  "main": "handler.js",
  "scripts": {
    "test": "serverless-bundle test",
    "test:get": "serverless invoke local --function get --path test/mocks/get-event.json",
    "test:create": "serverless invoke local --function create --path test/mocks/create-event.json",
    "test:list": "serverless invoke local --function list --path test/mocks/list-event.json",
    "test:update": "serverless invoke local --function update --path test/mocks/update-event.json",
    "test:delete": "serverless invoke local --function delete --path test/mocks/delete-event.json",
    "test:billing": "serverless invoke local --function billing --path test/mocks/billing-event.json"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-nodejs-starter.git"
  },
  "devDependencies": {
    "aws-sdk": "^2.782.0",
    "serverless-bundle": "^3.2.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.3.3"
  },
  "dependencies": {
    "stripe": "^8.119.0",
    "uuid": "^7.0.3"
  }
}
